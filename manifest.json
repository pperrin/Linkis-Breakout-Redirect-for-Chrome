{
  "name": "Linkis Breakout Redirect",
  "version": "0.3",
  "description": "Checks URL and redirects as required.",
  "background": { 
     "page":"bg.html"
     },
   "manifest_version": 2,
   "content_scripts": [
   {
     "matches": ["http://linkis.com/*", "https://linkis.com/*"],
     "js": ["content.js"]
   }
   ],
  "permissions": ["tabs"]
}